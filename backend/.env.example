# Configuration de la base de données
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USERNAME=postgres
# IMPORTANT: Le mot de passe doit être une chaîne de caractères entre guillemets s'il contient des caractères spéciaux
DATABASE_PASSWORD="your_password"
